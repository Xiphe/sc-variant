{
  "name": "sc-variant",
  "version": "0.1.0",
  "description": "tiny helper to create variants of styled components",
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "vrnt.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./vrnt.d.ts"
    }
  },
  "scripts": {
    "pretest": "markdown-to-test readme.md -o test",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "prepublish": "npm run test"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Xiphe/vrnt.git"
  },
  "keywords": [
    "styled-components",
    "variant",
    "theme",
    "props",
    "type-save"
  ],
  "author": "Hannes Diercks <node@xiphe.net> (https://xiphe.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xiphe/vrnt/issues"
  },
  "homepage": "https://github.com/Xiphe/vrnt#readme",
  "devDependencies": {
    "@types/jest": "28.1.7",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "5.1.26",
    "jest": "28.1.3",
    "markdown-to-test": "0.3.0",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.5",
    "ts-jest": "28.0.8",
    "typescript": "4.7.4"
  }
}
